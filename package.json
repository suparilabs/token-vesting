{
  "name": "token-vesting",
  "description": "Token vesting + ui",
  "version": "1.0.0",
  "devDependencies": {
    "@codechecks/client": "^0.1.12",
    "@commitlint/cli": "^16.1.0",
    "@commitlint/config-conventional": "^16.0.0",
    "@ethersproject/abi": "^5.5.0",
    "@ethersproject/abstract-signer": "^5.5.0",
    "@ethersproject/bignumber": "^5.5.0",
    "@ethersproject/bytes": "^5.5.0",
    "@ethersproject/providers": "^5.5.2",
    "@nomiclabs/hardhat-ethers": "npm:hardhat-deploy-ethers@0.3.0-beta.12",
    "@nomiclabs/hardhat-etherscan": "^3.0.0",
    "@nomiclabs/hardhat-waffle": "^2.0.2",
    "@typechain/ethers-v5": "^9.0.0",
    "@typechain/hardhat": "^4.0.0",
    "@types/chai": "^4.3.0",
    "@types/fs-extra": "^9.0.13",
    "@types/mocha": "^9.1.0",
    "@types/node": "^17.0.10",
    "@types/react": "17.0.38",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "chai": "^4.3.4",
    "commitizen": "^4.2.4",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "dotenv": "^14.2.0",
    "eslint": "^8.7.0",
    "eslint-config-next": "12.0.8",
    "eslint-config-prettier": "^8.3.0",
    "ethereum-waffle": "^3.4.0",
    "ethereumjs-abi": "^0.6.8",
    "ethers": "^5.5.4",
    "fs-extra": "^10.0.0",
    "hardhat": "^2.8.3",
    "hardhat-deploy": "^0.10.4",
    "hardhat-deploy-ethers": "^0.3.0-beta.13",
    "hardhat-gas-reporter": "^1.0.7",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.1",
    "lodash": "^4.17.21",
    "mocha": "^9.1.4",
    "pinst": "^2.1.6",
    "prettier": "^2.5.1",
    "prettier-plugin-solidity": "^1.0.0-beta.19",
    "shelljs": "^0.8.5",
    "shx": "^0.3.4",
    "solhint": "^3.3.6",
    "solhint-plugin-prettier": "^0.0.5",
    "solidity-coverage": "^0.7.18",
    "ts-generator": "^0.1.1",
    "ts-node": "^10.4.0",
    "typechain": "^7.0.0",
    "typescript": "^4.5.5"
  },
  "files": [
    "/contracts"
  ],
  "keywords": [
    "blockchain",
    "ethereum",
    "hardhat",
    "smart-contracts",
    "solidity"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "clean": "shx rm -rf ./artifacts ./cache ./coverage ./src/types ./coverage.json",
    "commit": "git-cz",
    "compile": "cross-env TS_NODE_TRANSPILE_ONLY=true hardhat compile",
    "coverage": "yarn typechain && hardhat coverage --solcoverjs ./.solcover.js --temp artifacts --testfiles \"./test/**/*.ts\"",
    "lint": "yarn lint:sol && yarn lint:ts && yarn prettier:check",
    "lint:sol": "solhint --config ./.solhint.json --max-warnings 0 \"contracts/**/*.sol\"",
    "lint:ts": "eslint --config ./.eslintrc.yaml --ignore-path ./.eslintignore --ext .js,.ts .",
    "postinstall": "husky install",
    "postpublish": "pinst --enable",
    "prepublishOnly": "pinst --disable",
    "prettier": "prettier --config ./.prettierrc.yaml --write \"**/*.{js,json,md,sol,ts}\"",
    "prettier:check": "prettier --check --config ./.prettierrc.yaml \"**/*.{js,json,md,sol,ts}\"",
    "test": "hardhat test --deploy-fixture",
    "typechain": "cross-env TS_NODE_TRANSPILE_ONLY=true hardhat typechain",
    "app:dev": "next dev",
    "app:build": "next build",
    "app:start": "next start",
    "app:lint": "next lint",
    "app:export": "yarn app:build && next export -o _static",
    "deploy:bsctestnet": "hardhat --network bsctestnet deploy",
    "deploy:bsc": "hardhat --network bsc deploy"
  },
  "dependencies": {
    "@ethersproject/address": "^5.5.0",
    "@ethersproject/contracts": "^5.5.0",
    "@ethersproject/units": "^5.5.0",
    "@metamask/detect-provider": "^1.2.0",
    "@metamask/onboarding": "^1.0.1",
    "@openzeppelin/contracts": "^4.4.2",
    "@uniswap/sdk": "^3.0.3",
    "@web3-react/core": "^6.1.9",
    "@web3-react/injected-connector": "^6.0.7",
    "@web3-react/network-connector": "^6.2.9",
    "autoprefixer": "^10.4.2",
    "bignumber.js": "^9.0.2",
    "bootstrap": "^5.1.3",
    "highcharts": "^9.3.2",
    "highcharts-react-official": "^3.1.0",
    "moment": "^2.29.1",
    "next": "12.0.8",
    "next-optimized-images": "^2.6.2",
    "papaparse": "^5.3.1",
    "postcss": "^8.4.5",
    "react": "17.0.2",
    "react-bootstrap": "^2.1.1",
    "react-chartjs-2": "^4.0.1",
    "react-countdown": "^2.3.2",
    "react-dom": "17.0.2",
    "react-toastify": "^8.2.0",
    "sharp": "^0.30.4",
    "swr": "^1.1.2"
  }
}
